{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "rules": {
        "react/jsx-filename-extension": "off",
        "react/prop-types": "off",
        "indent": [ "warn", 4, { "SwitchCase": 1, "ignoredNodes": [ "JSXAttribute", "JSXSpreadAttribute" ] }],
        "react/jsx-indent": ["error", 4]

    },
    "globals": {
        "API_ENDPOINT": true
    }
}